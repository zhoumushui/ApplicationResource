CREATE TABLE material_online(
	_id integer PRIMARY KEY ,
	material_id TEXT  NOT NULL,
	material_name TEXT  ,
	material_enname TEXT  ,
	category_id TEXT  NOT NULL,
	sub_category_id text  ,
	sub_category_name text  ,
	preview_path text  ,
	thumbnail_path text  ,
	status integer   DEFAULT('0'),
	vip integer   DEFAULT('0'),
	version integer  ,
	type_order integer  ,
	minversion integer  ,
	maxversion integer  ,
	material_thumbnail_url text  ,
	material_has_preview int  NOT NULL DEFAULT('0'),
	material_preview_url text  ,
	has_ad int  NOT NULL DEFAULT('0'),
	ad_id text  ,
	material_url text  ,
	is_new INTEGER  NOT NULL DEFAULT(0),
	photo_amount INTEGER  NOT NULL DEFAULT(0)
)